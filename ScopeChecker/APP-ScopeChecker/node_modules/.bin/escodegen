#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/C:/Users/chen'yu/Desktop/ScopeChecker-main/node_modules/.store/escodegen@2.1.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:$basedir/C:/Users/chen'yu/Desktop/ScopeChecker-main/node_modules/.store/escodegen@2.1.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.store/escodegen@2.1.0/node_modules/escodegen/bin/escodegen.js" "$@"
else
  exec node  "$basedir/../.store/escodegen@2.1.0/node_modules/escodegen/bin/escodegen.js" "$@"
fi
