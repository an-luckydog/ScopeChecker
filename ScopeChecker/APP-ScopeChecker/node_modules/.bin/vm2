#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/C:/Users/chen'yu/Desktop/ScopeChecker-main/node_modules/.store/vm2@3.9.19/node_modules"
else
  export NODE_PATH="$NODE_PATH:$basedir/C:/Users/chen'yu/Desktop/ScopeChecker-main/node_modules/.store/vm2@3.9.19/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.store/vm2@3.9.19/node_modules/vm2/bin/vm2" "$@"
else
  exec node  "$basedir/../.store/vm2@3.9.19/node_modules/vm2/bin/vm2" "$@"
fi
